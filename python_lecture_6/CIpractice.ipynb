    "import numpy as np\n",
    "n = 60\n",
    "xbar = 1000\n",
    "stdev = 200\n",
    "Z = 1.96\n",
    "\n",
    "lower_ci = xbar - Z*(stdev/np.sqrt(stdev))\n",
    "upper_ci = xbar + Z*(stdev/np.sqrt(stdev))\n",
    "\n",
    "print(f\"95% Con Interval ({lower_ci}, {upper_ci})\")"
